<div id="set" class="formfull">
    <input name="Experience" id="Experience" onclick="addform(this)" type="submit" value="ADD" class="addbutton addprofile">
    <h1 style="margin-top:10px;">Experience Set</h1>
    <br>
    <hr>
    <div class="finalset">
        <form id="Experienceform" style=" border:1px; text-align:right; display:none;">
            <img src="assets/img/close.png" style="cursor:pointer;" onclick="hideform(this)" name="Experience">
            <input type="text" placeholder="COMPANY NAME " class="formfull">
            <input type="text" placeholder="JOB POSITION " class="formfull">
            <input type="text" placeholder="LOCATION " class="formfull">
            <input placeholder="START DATE" type="text" id="startdate" class="formfull hasDatepicker">
            <input placeholder="END DATE" type="text" id="enddate" class="formfull hasDatepicker">
            <input type="submit" value="SUBMIT" class="addbutton">
            <script>
                $(function () {
                    $("#startdate").datepicker();
                });
                $(function () {
                    $("#enddate").datepicker();
                });
                $(function () {
                    $("#dob").datepicker();
                });
            </script>
        </form>
    </div>
    <div class="finalset">
        <p class="companydata "><span class="text">Company Name</span></p>
        <input type="submit" name="Experience" id="Experience1" onclick="addform(this)" value="EDIT" class="addbutton editprofile" style="display: none;">
        <p class="companydata "><span class="text">Job Position</span></p>
        <p class="companydata "><span class="text">Location</span></p>
        <p class="companydata "><span class="text">Start Date To End Date</span></p>
    </div>

    <hr>
    <div class="finalset" id="skillsetsave">
        <p class="editablelabel companydata "><span class="text">Company Name</span></p>
        <input type="submit" name="Experience" id="Experience1" onclick="addform(this)" value="EDIT" class="addbutton editprofile" style="display: none;">
        <p class="editablelabel companydata "><span class="text">Job Position</span></p>
        <p class="editablelabel companydata "><span class="text">Location</span></p>
        <p class="editablelabel companydata "><span class="text">Start Date To End Date</span></p>
    </div>
    <hr>
</div>
